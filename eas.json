{
  "cli": {
    "version": ">= 0.38.3"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "releaseChannel": "development",
      "env": {
        "APP_ENV": "development"
      },
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "simulator": {
      "distribution": "internal",
      "releaseChannel": "dev",
      "ios": {
        "simulator": true
      }
    },
    "staging": {
      "releaseChannel": "beta",
      "env": {
        "APP_ENV": "staging"
      }
    },
    "production": {
      "releaseChannel": "prod",
      "env": {
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
